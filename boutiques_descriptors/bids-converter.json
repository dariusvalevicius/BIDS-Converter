            "value-key": "[INPUT]",
            "command-line-flag": "--input"
        },
        {
            "id": "output_prefix",
            "name": "Output Folder Prefix",
            "description": "String to be added to [Input] to create top-level output folder.",
            "optional": true,
            "type": "String",
            "value-key": "[OUTPUT_PREFIX]",
            "command-line-flag": "--output_prefix"
